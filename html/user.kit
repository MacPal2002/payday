        <div class="l-column-left">
            <!-- @include 'components/balance.kit' -->

            <section class="c-subscriptions" id="subscriptions">
                <p class="c-subscriptions__header">Zarządzaj <br>subskrypcjami</p>
                <div class="c-subscriptions__heading-box">
                    <h2>Twoje subskrypcje</h2>
                    <p><span class="u-emphasize">16 zł</span> miesięcznie</p>
                </div>
                <div class="l-cards">
                    <div class="c-card">
                        <div class="c-card__title-box">
                            <div class="c-icon-box c-icon-box--accent-faded">
                                <div class="c-icon-box__icon"
                                    style="mask: url(../dist/public/img/icons/netflix.min.svg)">
                                </div>
                            </div>
                            <p>Netflix</p>
                        </div>

                        <div class="c-card__price-box">
                            <p><span class="u-emphasize--faded">16 zł</span> /miesiąc</p>
                            <p>Płatność za 15 dni</p>
                        </div>
                        <p class="c-card__action"><a href="#">Anuluj subskrypcje</a></p>
                    </div>
                    <div class="c-card">
                        <div class="c-card__title-box">
                            <div class="c-icon-box c-icon-box--accent-faded">
                                <div class="c-icon-box__icon"
                                    style="mask: url(../dist/public/img/icons/spotify.min.svg)">
                                </div>
                            </div>
                            <p>Spotify</p>
                        </div>

                        <div class="c-card__price-box">
                            <p><span class="u-emphasize--faded">8 zł</span> /miesiąc</p>
                            <p>Płatność co miesiąc</p>
                        </div>
                        <p class="c-card__action"><a href="#">Dodaj subskrypcje</a></p>
                    </div>
                    <div class="c-card">
                        <div class="c-card__title-box">
                            <div class="c-icon-box c-icon-box--accent-faded">
                                <div class="c-icon-box__icon"
                                    style="mask: url(../dist/public/img/icons/disney.min.svg)">
                                </div>
                            </div>
                            <p>Disney+</p>
                        </div>

                        <div class="c-card__price-box">
                            <p><span class="u-emphasize--faded">7 zł</span> /miesiąc</p>
                            <p>Płatność co miesiąc</p>
                        </div>
                        <p class="c-card__action"><a href="#">Dodaj subskrypcje</a></p>
                    </div>
                </div>
            </section>
        </div>
